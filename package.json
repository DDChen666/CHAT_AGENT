{
  "name": "synapse-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build":"prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate",
    "deploy": "git add . && git commit -m 'Deploy update' && git push origin main",
    "preview": "npm run build && npm run start",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "echo 'No unit tests configured yet'",
    "test:integration": "npm run db:test-connection && npm run build:test",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:test-connection": "tsx scripts/test-db-connection.ts",
    "build:test": "npm run lint && npm run type-check && npm run build",
    "type-check": "tsc --noEmit",
    "quality-check": "npm run lint && npm run type-check && npm run test:integration",
    "pre-commit": "npm run quality-check",
    "clean": "rm -rf .next && rm -rf node_modules/.cache"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-toast": "^1.2.15",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "docx": "^9.5.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.541.0",
    "marked": "^16.2.1",
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "rehype-sanitize": "^6.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/file-saver": "^2.0.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dexie": "^4.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "prisma": "^5.20.0",
    "sharp": "^0.34.3",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
